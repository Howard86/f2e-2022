{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "build:docs": "turbo run build --filter=docs",
    "build:animated-landing": "turbo run build --filter=animated-landing",
    "build:pdf": "turbo run build --filter=pdf",
    "build:scrum": "turbo run build --filter=scrum",
    "dev:docs": "turbo run dev --filter=docs --no-cache --parallel --continue",
    "dev:animated-landing": "turbo run dev --filter=animated-landing --no-cache --parallel --continue",
    "dev:pdf": "turbo run dev --filter=pdf --no-cache --parallel --continue",
    "dev:scrum": "turbo run dev --filter=scrum --no-cache --parallel --continue",
    "start:docs": "turbo run start --filter=docs",
    "start:animated-landing": "turbo run start --filter=animated-landing",
    "start:pdf": "turbo run start --filter=pdf",
    "start:scrum": "turbo run start --filter=scrum",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write --list-different \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version && pnpm install --lockfile-only",
    "release": "turbo run build --filter=docs^... && changeset publish",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@changesets/cli": "^2.25.2",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@next/bundle-analyzer": "^13.0.5",
    "@playwright/test": "^1.33.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.2.3",
    "@types/testing-library__jest-dom": "^5.14.5",
    "canvas": "^2.10.2",
    "esbuild": "^0.15.15",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-f2e-2022": "workspace:*",
    "eslint-config-next": "^13.0.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-tailwindcss": "^3.7.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.4",
    "next": "^13.0.5",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-html-props": "^2.0.0",
    "tsup": "^6.5.0",
    "turbo": "^1.6.3",
    "typescript": "^5.0.0"
  }
}
